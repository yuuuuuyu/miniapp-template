{
    "name": "miniprogram-ts-less-quickstart",
    "version": "1.2.10",
    "description": "小程序快速开发模板",
    "scripts": {
        "preview": "node cli/index.js preview",
        "upload": "node cli/index.js upload",
        "build-npm": "node cli/index.js build-npm",
        "ci:help": "node cli/index.js help",
        "commit": "git add . && git-cz && git push",
        "prepare": "husky install",
        "setup-husky": "sh scripts/setup-husky.sh",
        "demo:changelog": "node scripts/demo-changelog-formats.js",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix"
    },
    "keywords": [],
    "author": "",
    "license": "",
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "devDependencies": {
        "@babel/helper-string-parser": "^7.27.1",
        "@babel/helper-validator-identifier": "^7.27.1",
        "@commitlint/cli": "^19.8.1",
        "@commitlint/config-conventional": "^19.8.1",
        "@types/node": "^24.0.3",
        "@typescript-eslint/eslint-plugin": "^8.34.1",
        "@typescript-eslint/parser": "^8.34.1",
        "chalk": "4",
        "commitizen": "^4.3.1",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^9.29.0",
        "eslint-config-prettier": "^10.1.5",
        "husky": "^9.1.7",
        "inquirer": "^8.2.6",
        "miniprogram-api-typings": "^2.8.3-1",
        "miniprogram-ci": "^2.1.14",
        "to-fast-properties": "2.0.0",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "@vant/weapp": "^1.11.6",
        "ansi-styles": "^6.2.1",
        "supports-color": "^10.0.0"
    }
}